                         <script type="text/javascript" src="//csdnimg.cn/release/cart/pub/cart_jsonp.js"></script>
            <script src="//csdnimg.cn/release/download/old_static/js/qrcode.js?v="></script>

            <script>
                $(function(){
                    function query_order_info() {
                        $("#notify").hide();
                        $("#already_open_tips").hide();
                        $('.vip_buy_t_r .vip_pay .member_discount').hide();
                        $("#union_payment").parents("#vip_list_fir").hide();

                        $("#wechat_payment").addClass("grey_border");
                        $("#wechat_payment").html('<img class="loading" src="//csdnimg.cn/release/download/images/loading.png"/>');
                        cart.scanPay({
                            product_id: , // 产品id 可选
                            goods_id: , // 商品id 必填
                            num: 1, // 购买数量 可选，默认1
                            request_type: 4,// 请求类型 1、仅请求支付宝二维码 2、仅请求微信二维码 3、同时请求支付宝和微信二维码 4、返回合并后的二维码，支付宝和微信都可以扫
                            flag: 1, // 业务线
                            order_source:6,//收入归属产品
                            success_function: show_pay_code,// 成功回调方法名
                            error_function: show_pay_error,// 失败回调方法名
                            timeout_function: show_pay_timeout,// 超时回调方法名
                            use_cache: true,// true为使用缓存，为空或其他不使用缓存
                            anonymous_purchase: 1,
                            ext_param_json: '{"download_source_id":6762849}'
                        });
                    }
                    $(document).on('click', '#notify', function(){
                        query_order_info()
                    })
                    query_order_info();

                     //生成二维码
                    function generate_code(container_id, code_url) {
                        //参数1表示图像大小，取值范围1-10；参数2表示质量，取值范围'L','M','Q','H'
                        var qr = qrcode(6, 'M');
                        qr.addData(code_url);
                        qr.make();
                        $("#" + container_id).html(qr.createImgTag(3, 3));

                        $("#" + container_id).html($("#" + container_id).html());

                    }

                    function show_pay_code(type, data) {
                        if (data.status != 1) {
                            exchangeHtml('pay_time_out', '点击重新获取');
                            return false;
                        }
                        generate_code('wechat_payment', data.pay_url);
                        $("#union_payment").parents("#vip_list_fir").show();
                        $("#union_payment").attr("href", data.pay_url);
                        if (data.price > 0) {
                            $('.vip_buy_t_r .vip_pay strong').html(data.price + '元');
                            if (data.member_discount > 0) {
                                $('.vip_buy_t_r .vip_pay .member_discount').show().text("会员续费" + data.member_discount + "折");
                            }
                        }
                    }

                    function show_pay_error(data) {
                        //exchangeHtml('pay_error','获取失败，点击重试');
                        $("#union_payment").parents("#vip_list_fir").hide();
                        if (window.console && console.log) {
                            show_pay_timeout();
                        }
                    }

                    function show_pay_timeout() {
                        $("#union_payment").parents("#vip_list_fir").hide();
                        exchangeHtml('pay_time_out', '点击重新获取');
                    }

                    function exchangeHtml(img, tips) {
                        $("#notify").show().html('<img class="pay_icon" src="//csdnimg.cn/release/download/images/' + img + '.png"/><span class="pay_tip">' + tips + '</span>');
                        $("#wechat_payment").html('<img src="//csdnimg.cn/public/static/img/csdn-userimg250.gif" width="150" height="150"/>');
                    }
                })
            </script>
            <!-- 2019.3.13 增加送书 end -->

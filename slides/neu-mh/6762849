            <script type="text/javascript">
                function check_download_code() {
                    var download_code = $('#download_code').val();
                    var download_code = download_code.toString();
                    $.ajax({
                        type: "get",
                        url: "/index.php/source/check_validatetaskdlcode?dlcode=" + download_code,
                        data: {},
                        success: function (data) {
                            data = JSON.parse(data);
                            if (data.status) {
                                $('#noVipZeroP .download_code_err').html('');
                                $('#noVipZeroP .resource_dl_btn').find('a').show();
                            } else {
                                $('#noVipZeroP .download_code_err').html(data.error);
                                $('#noVipZeroP .resource_dl_btn').find('a').hide();
                            }
                        },
                        error: function (err) {
                            console.log(err);
                        }
                    });
                }
            </script>

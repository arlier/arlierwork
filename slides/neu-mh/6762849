<script type="application/javascript">
    (function () {
        window._smConf = {
            organization: 'E2fSjOLnvVdMRc4jLSih',
            staticHost: 'static.fengkongcloud.com'
        };
        var url = (function () {
            var originHost = "static2.fengkongcloud.com";
            var isHttps = 'https:' === document.location.protocol;
            var protocol = isHttps ? 'https://' : 'http://';
            var fpJsPath = '/fpv2.js';
            var url = protocol + _smConf.staticHost + fpJsPath;
            var ua = navigator.userAgent.toLowerCase();
            var isWinXP = /windows\s(?:nt\s5.1)|(?:xp)/.test(ua);
            var isLowIE = /msie\s[678]\.0/.test(ua);
            if (isHttps && isWinXP && isLowIE) {
                url = protocol + originHost + fpJsPath;
            }
            return url;
        })();
        var sm = document.createElement("script");
        var s = document.getElementsByTagName("script")[0];
        sm.src = url;
        s.parentNode.insertBefore(sm, s);
    })();
    $(function () {
        //右侧全部固定在底部
        window.csdn.fixedSidebar({
            targetBox: $('.public_pc_right_footer2018'), //判断到达底部的div,公共footer可选 $('.persion_article'),$('.public_pc_right_footer2018')
            mainBox: $(".download_l"), //侧边栏旁边的主体div,默认 $("main")
            sidebar: $(".dl_right_fixed"), //侧边栏,默认 $("aside")
            direction: 'right', //侧边栏浮动方向,可选'right','left',
            position: 'fixed',//定位方式
            bottom: 0,
            zIndex: 99,
            sidebarRightMargin: 20,//direction为left,侧边栏距离mainbox的右边距
            sidebarLeftMargin: 20,//direction为right,侧边栏距离mainbox的左边距
        })

        // 版权跳转
        function coptyRedirect() {
            setTimeout(function () {
                //触发点击事件
                $("#source_101_order_click").trigger('click')
                //跳转订单页面
                go_mail()
            }, 3000)
        }
    });
</script>

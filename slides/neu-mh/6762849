<script type="text/javascript" defer>
    function get_find_report(bd, pageno) {
        $.get("/index.php/source/get_source_report/" + bd + "/" + pageno,
            function (data) {
                if (data.succ > 0) {
                    var html = '';
                    for (var i = 0; i < data.info.length; i++) {
                        html += '        <li><i class="fa fa-caret-right"></i>';
                        html += '            <a strategy="SearchAlgorithm" title="' + data.info[i].object.title + '" href="' + data.info[i].object.url + '" target="_blank">' + data.info[i].object.title + '</a><span>积分：<em>' + data.info[i].object.sourcescore + '</em></span>';
                        html += '        </li>';
                    }
                    var pre = (pageno - 1) < 1 ? data.total_rows : (pageno - 1);
                    var next = (pageno + 1) > data.total_rows ? 1 : (parseInt(pageno) + 1);
                    $(".fa-angle-left").attr("data-id", pre);
                    $(".fa-angle-right").attr("data-id", next);
                    $("#over_download").html(html);
                }
            },
            'json');
    }

    var title = $('.download_dl').find('h3').attr('title');
    var $tags = $('.dl_b').find('a.tag');
    var bd, tags = [];

    $tags.each(function (i, tag) {
        tags.push($(tag).html());
    });
    bd = title + ',' + tags.join(',');
    var pageno = 1;
    $(document).ready(function () {

        $(".fa-angle-left").click(function () {
            pageno = $(".fa-angle-left").attr("data-id");
            get_find_report(bd, pageno);
        });
        $(".fa-angle-right").click(function () {
            pageno = $(".fa-angle-right").attr("data-id");
            get_find_report(bd, pageno);
        });
    });

    /**
     vip会员动态
     **/
    $(document).ready(function () {
        $.ajax({
            type: 'get',
            url: '/index.php/source/up_vip_users?web=web',
            async: false,
            dataType: 'jsonp',
            jsonp: 'callback',
            success: function (resobj) {
                var oHtml = '';
                for (var i = 0; i < resobj.length; i++) {
                    resobj[i].link = "http://my.csdn.net/" + resobj[i].username;
                    oHtml += '<li><a target="_blank" href="' + resobj[i].link + '" title="' + resobj[i].username + '" class="vip_name">' + resobj[i].nickname + '</a><span>' + resobj[i].vip_type + '</span></li>';
                }
                $(".vip_dynamic").append(oHtml);
            },
            error: function (error) {
                console.log(error);
            }
        });
        (function ($) {
            $.fn.extend({
                Scroll: function (opt, callback) {
                    //参数初始化
                    if (!opt)
                        var opt = {};
                    var _this = this.eq(0).find("ul:first");
                    var lineH = _this.find("li:first").height(), //获取行高
                        line = opt.line ? parseInt(opt.line, 10) : parseInt(this.height() / lineH, 10), //每次滚动的行数，默认为一屏，即父容器高度
                        speed = opt.speed ? parseInt(opt.speed, 10) : 500, //卷动速度，数值越大，速度越慢（毫秒）
                        timer = opt.timer ? parseInt(opt.timer, 10) : 4000; //滚动的时间间隔（毫秒）
                    if (line == 0)
                        line = 1;
                    var upHeight = 0 - line * lineH;
                    //滚动函数
                    scrollUp = function () {
                        _this.animate({
                            marginTop: upHeight
                        }, speed, function () {
                            for (i = 1; i <= line; i++) {
                                _this.find("li:first").appendTo(_this);
                            }
                            _this.css({marginTop: 0});
                        });
                    };
                    //鼠标事件绑定
                    _this.hover(function () {
                        clearInterval(timerID);
                    }, function () {
                        timerID = setInterval("scrollUp()", timer);
                    }).mouseout();
                }

            });
        })(jQuery);

        //ajax先填充内容，再滚动
        setTimeout(function () {
            $("#vip_dynamic").Scroll({line: 1, speed: 500, timer: 2000});
        }, 1000);

    })
</script>


<script type="text/javascript">
    $(function () {
        var book_url = $(".book_url");
        var sourceId = $("#source_id").val();
        var userName = $("#report_username").val();

        $("#do_addC").click(function () {
            _hmt.push(['_setAutoPageview', false]);
            _hmt.push(['_trackPageview', '/do_addC/' + userName + '/' + sourceId]);
        });

        $(".ask_column").click(function () {
            _hmt.push(['_setAutoPageview', false]);
            _hmt.push(['_trackPageview', '/ask_column/' + userName]);
        });

        //相关推荐  立即下载
        $(".dl_reco_btn").click(function () {
            _hmt.push(['_setAutoPageview', false]);
            _hmt.push(['_trackPageview', '/dl_recommend/']);
        });

        $("#ask_more").click(function () {
            _hmt.push(['_setAutoPageview', false]);
            _hmt.push(['_trackPageview', '/ask_more/' + userName]);
        });

        $("#do_openV").click(function () {
            _hmt.push(['_setAutoPageview', false]);
            _hmt.push(['_trackPageview', '/do_openV/' + userName + '/' + sourceId]);
        });
        //封杀用户弹窗
        $(".user_black").click(function () {
            alert("您的账号存在异常，请您联系站内客服解决，谢谢");
        })
        //盗版图书跳转
        $(".grey_dl_btn_null").click(function () {
            if ($(".grey_dl_btn_null").attr("book_url") != "") {
                this.target = "_blank"
                this.href = $(".grey_dl_btn_null").attr("book_url")
            }
        })
        //统计正版图书点击量
        $(".book_clicks").click(function () {
            var url = "/index.php/source/add_book_clicks";
            var book_url = $(this).attr("book_url");
            var url_str = "gitbook.cn/gitchat/geekbook";
            if (book_url.indexOf(url_str) != -1) {
                $.post(url, {book_url: book_url});
            }
        })
        //统计内部上报无版权资源旁边购买vip点击信息
            })

</script>

<script type="text/javascript">
    $(function () {
        //start 按钮置灰则加入百度统计  hidden 为置灰 show为正常
        var souce_status = 2;
        var status_101 = 101;
        if (souce_status == status_101) {
            _hmt.push(['_setPageTag', '3804', 'hidden']);
        } else {
            _hmt.push(['_setPageTag', '3804', 'show']);
        }
        //end
        _hmt.push(['_setAutoPageview', false]);
        _hmt.push(['_setUserTag', '1788', "PC_VC"]);
    });
</script>


